<% @page_title = "Edit Subject" %>

<%= link_to("<< Back to List", subjects_path, :class => 'back_link') %>

<div class="subjects edit">
  <H2>Edit Subject</H2>

  <%= form_for(@subject) do |f| %>

      <%= render(:partial => 'form', :locals => {:f => f}) %>

      <div class="form-buttons">
        <%= f.submit("Update Subject") %>
      </div>

  <% end %>

</div>
